program laba4;
var y,q,p,i:real;
x,e:integer;
begin 
    writeln('введите x =' );
    read(x);
    writeln('введите e =' );
    read(e);
    q := 4.1;
    p := 1.3;
    begin
        for i:= 0 to 2.2 do
        y := abs(sqr(cos)*q*x) * exp(e * ln(-p*x));
        i := i + 0.23;
    end    
    writeln('Ответ y =', y);
end
